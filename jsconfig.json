{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["src/*"],
      "@vab/*": ["library/*"],
      "@gp": ["library/plugins/vab"]
    },
    "lib": [
      "dom",
      "esnext",
      "scripthost",
      "dom.iterable"
    ]
  },
  "exclude": ["node_modules"]
}
